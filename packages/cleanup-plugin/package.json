{
    "name": "@playkostudios/cleanup-plugin",
    "version": "0.0.1",
    "description": "Wonderland Editor plugin to cleanup resources in Wonderland Engine projects.",
    "main": "index.mjs",
    "typings": "index.d.ts",
    "type": "module",
    "module": "index.mjs",
    "keywords": [
        "wonderland-engine"
    ],
    "scripts": {
        "build": "esbuild index.ts --bundle --minify --platform=node --format=esm --target=es2022 --outfile=../playkostudios-cleanup-plugin.mjs",
        "tsc": "tsc --noEmit"
    },
    "prettier": "@wonderlandengine/prettier-config",
    "dependencies": {
        "@types/node": "^22.15.18",
        "@wonderlandcloud/cli": "^0.2.22",
        "@wonderlandengine/editor-api": "^1.4.0"
    },
    "devDependencies": {
        "@wonderlandengine/prettier-config": "^1.0.0",
        "typescript": "^5.8.3"
    }
}